{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.41",
    "apollo-server": "^2.11.0",
    "formik": "^2.1.5",
    "gatsby": "^2.19.45",
    "gatsby-image": "^2.2.44",
    "gatsby-plugin-apollo": "^3.0.1",
    "gatsby-plugin-manifest": "^2.2.48",
    "gatsby-plugin-offline": "^3.0.41",
    "gatsby-plugin-react-helmet": "^3.1.24",
    "gatsby-plugin-sharp": "^2.4.13",
    "gatsby-source-filesystem": "^2.1.56",
    "gatsby-transformer-sharp": "^2.3.19",
    "graphql": "^14.6.0",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "storybook": "^6.0.18",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storyshots": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@testing-library/cypress": "^6.0.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "chai": "^4.2.0",
    "cypress": "^5.0.0",
    "cypress-cucumber-preprocessor": "^2.5.5",
    "cypress-screenplay": "^0.1.0",
    "istanbul-merge": "^1.1.1",
    "jest": "25",
    "jest-css-modules": "^2.1.0",
    "loki": "^0.20.3",
    "mocha": "^7.1.1",
    "nyc": "^15.1.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.13.1",
    "sinon": "^9.0.2",
    "start-server-and-test": "^1.11.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "api": "node src/api/index.js",
    "test:jest-update": "jest -u",
    "test:jest": "jest --coverage",
    "test:integration": "start-test api http-get://localhost:4000/?query=%7B__typename%7D develop 8000 cypress:run",
    "coverage:clear": "rm -rf .nyc_output coverage .coverage && mkdir -p .coverage/merged ",
    "coverage:merge": "yarn istanbul-merge --out .coverage/merged/coverage.json .nyc_output/coverage-final.json",
    "coverage:report": "nyc report --reporter=lcov --reporter=text --reporter=html --temp-dir=.coverage/merged --report-dir=.coverage/merged",
    "test": "yarn coverage:clear && yarn test:jest && yarn test:integration && yarn coverage:merge && yarn coverage:report",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  },
  "nyc": {
    "exclude": [".storybook", "src/external"]
  }
}
